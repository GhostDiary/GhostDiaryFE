<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <stroke android:color="#474746" android:width="2dp"/>
            <corners
                android:radius="10dp"/>
            <padding android:bottom="10dp" android:left="14dp" android:right="14dp" android:top="10dp">
            </padding>
        </shape>
    </item>
</layer-list>